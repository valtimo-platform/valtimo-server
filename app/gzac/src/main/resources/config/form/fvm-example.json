                                {
                                    "key": "enableSubmissionValidation",
                                    "type": "checkbox",
                                    "input": true,
                                    "label": "Enable submission validation",
                                    "tableView": false,
                                    "defaultValue": false,
                                    "validateWhenHidden": false
                                },
                                {
                                    "key": "enableBusinessValidation",
